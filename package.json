{
  "name": "glennallen-panthers-newsletter",
  "version": "1.0.0",
  "description": "🐾 A fierce newsletter generator with CLAWS and attitude",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "node scripts/copy-assets.js",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "generate": "npm run build && node dist/cli.js generate",
    "netlify-build": "npm run build && node dist/cli.js generate --data data/sample-week.json --output out"
  },
  "keywords": ["newsletter", "education", "panthers", "fierce", "dynamic"],
  "author": "TheAccidentalTeacher",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "handlebars": "^4.7.8",
    "puppeteer": "^21.5.2",
    "axios": "^1.6.2",
    "sharp": "^0.32.6",
    "yaml": "^2.3.4",
    "qrcode": "^1.5.3",
    "chalk": "^4.1.2",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0"
  }
}
